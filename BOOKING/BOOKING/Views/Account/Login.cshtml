@model BOOKING.Models.Register

<h2>Logowanie</h2>

@using (Html.BeginForm("Login", "Account", FormMethod.Post))
{
    @Html.ValidationSummary();

    <p>Login:  </p> @Html.TextBoxFor(x => x.userName)
    <p>Hasło:  </p> @Html.PasswordFor(x => x.Password)

    <p><button type="submit">Zaloguj</button></p>

}